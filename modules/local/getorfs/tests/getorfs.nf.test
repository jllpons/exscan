nextflow_process {

    name "Test Process GETORFS"
    script "../main.nf"
    process "GETORFS"

    test("Testing process GETORFS on *E. coli* LexA...") {

        when {
            params {
                outdir = "tests/results"
            }
            process {
                """
                input[0] = file("${projectDir}/data/fasta/lexA_Ecoli.fasta")
                """
            }
        }

        then {
            assert process.success
            assert snapshot(process.out).match()
        }

    }

}

